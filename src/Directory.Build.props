<?xml version="1.0" encoding="utf-8"?>
<Project>

  <PropertyGroup>
    
    <!-- Informative  -->
    <Authors>Anthony Marmont and Contributors</Authors>
    <Copyright>Copyright Â© Anthony Marmont and Contributors</Copyright>
    <Company>RPGCore</Company>
    <Product>RPGCore</Product>
    <VersionPrefix>0.1.0</VersionPrefix>
    <VersionSuffix>alpha</VersionSuffix>

    <EnableDefaultNoneItems>false</EnableDefaultNoneItems>
    <SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageLicenseExpression>CC-BY-NC-4.0</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>

    <PackageIcon>project-icon_rounded_64x64.png</PackageIcon>
    <RepositoryUrl>https://github.com/Fydar/RPGCore</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Fydar/RPGCore</PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/Fydar/RPGCore/master/src/project-icon_rounded_64x64.png</PackageIconUrl>

    <!-- Compilation -->
    <LangVersion>latest</LangVersion>
    <!--<Nullable>enable</Nullable>-->
    <Features>strict</Features>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <!--<TreatWarningsAsErrors>true</TreatWarningsAsErrors>-->
    <MinClientVersion>4.3</MinClientVersion>
    <UseSharedCompilation>true</UseSharedCompilation>
    
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)RPGCore.snk</AssemblyOriginatorKeyFile>
    <AssemblyOriginatorPublicKey></AssemblyOriginatorPublicKey>
    
    <!-- Output  -->
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <OverwriteReadOnlyFiles>true</OverwriteReadOnlyFiles>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
    <DebugType>embedded</DebugType>

    <BaseArtifactsPath>$(MSBuildThisFileDirectory)artifacts/</BaseArtifactsPath>
    <BaseArtifactsPathSuffix>$(MSBuildProjectName)</BaseArtifactsPathSuffix>

    <BaseOutputPath>$(BaseArtifactsPath)bin/$(BaseArtifactsPathSuffix)/</BaseOutputPath>
    <BaseIntermediateOutputPath>$(BaseArtifactsPath)obj/$(BaseArtifactsPathSuffix)/</BaseIntermediateOutputPath>
    <PackageOutputPath>$(BaseArtifactsPath)pkg/$(Configuration)/</PackageOutputPath>

  </PropertyGroup>

</Project>